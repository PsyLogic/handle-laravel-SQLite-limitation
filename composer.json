{
	"name": "psylogic/laravel-sqlite-handler",
	"description": "Handle Migrations limitation while testing",
	"keywords": ["Laravel","SQLite"],
	"license": "MIT",
	"authors": [
		 {
			  "name": "Othmane Nemli",
			  "email": "nemli.othmane@gmail.com"
		 }
	],
	"minimum-stability": "dev",
	"require": {
		"php": ">=7.1.3"
	},
	"autoload": {
		 "psr-4": {
			  "Psylogic\\SQLite\\": "src/",
			  "Psylogic\\SQLite\\Test\\": "tests/"
		 }
	},
	"extra": {
		 "laravel": {
			  "providers": [
					"Psylogic\\SQLite\\SQLiteServiceProvider"
			  ]
		 }
	},
	"require-dev": {
		"orchestra/testbench": "5.x-dev",
		"doctrine/dbal": "^2.6"
	}
}
